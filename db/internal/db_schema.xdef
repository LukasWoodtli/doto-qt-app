

TABLE(TASKS)
    COLUMN(NAME, QString)
    COLUMN(DONE, bool)
    COLUMN(DUE_DATE, QDate)
ENDTABLE()

TABLE(TASK_LISTS)
    COLUMN(TASK, QString)
    FOREIGN_KEY(TASK, TASKS, UUID)
ENDTABLE()

TABLE(SETTINGS)
    COLUMN(NAME, QString, UNIQUE)
    COLUMN(VALUE, QString)
    COLUMN(CPP_TYPE, QString)
ENDTABLE()